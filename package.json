{
  "name": "flatris",
  "version": "1.0.0",
  "description": "Tetris for millennials",
  "repository": {
    "type": "git",
    "url": "https://github.com/skidding/flatris.git"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "raf": "^3.3.0",
    "react": "^15.4.2",
    "react-animation-loop": "^0.1.2",
    "react-component-tree": "^0.4.0",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "prettier": "^0.18.0",
    "react-cosmos-redux-proxy": "^2.0.0-beta.6",
    "react-cosmos-telescope": "^2.0.0-beta.7",
    "react-cosmos-webpack": "^2.0.0-beta.7",
    "react-scripts": "0.9.0",
    "webpack": "^2.2.1"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "format": "prettier --write 'src/**/*.js' '*.js' 'cosmos/**/*.js' --single-quote",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "cosmos": "NODE_ENV=development cosmos"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
